20}},{"generated":{"line":131,"column":8},"source":"projects/music-master/components/Tracks.js","original":{"line":33,"column":12}},{"generated":{"line":131,"column":9},"source":"projects/music-master/components/Tracks.js","original":{"line":32,"column":8}},{"generated":{"line":132,"column":0},"source":"projects/music-master/components/Tracks.js","original":{"line":48,"column":5}},{"name":"Component","generated":{"line":136,"column":2},"source":"projects/music-master/components/Tracks.js","original":{"line":3,"column":21}},{"name":"Tracks","generated":{"line":138,"column":15},"source":"projects/music-master/components/Tracks.js","original":{"line":51,"column":15}}],"sources":{"projects/music-master/components/Tracks.js":"import React, { Component } from 'react';\r\n\r\nclass Tracks extends Component {\r\n    state = { playing: false, audio: null, playingPreviewURL: null };\r\n    playAudio = previewURL => () => {\r\n            const audio = new Audio(previewURL);\r\n            if(!this.state.playing){\r\n                audio.play();\r\n                this.setState({ playing: true, audio, playingPreviewURL: previewURL });\r\n            } else {\r\n                this.state.audio.pause();\r\n                if(this.state.playingPreviewURL === previewURL){\r\n                    this.setState({ playing: false });\r\n                } else {\r\n                    audio.play();\r\n                    this.setState({ audio, playingPreviewURL: previewURL });                \r\n                }\r\n            } \r\n    }\r\n    trackIcon = track =>{\r\n        if(!track.preview_url){\r\n            return <span>N/A</span>;\r\n        }\r\n        if(this.state.playing && this.state.playingPreviewURL === track.preview_url){\r\n            return <span>| |</span>;\r\n        } else {\r\n            return <span>&#9654;</span>;\r\n        }        \r\n    }\r\n    render(){\r\n        const { tracks } = this.props;\r\n        return(\r\n            <div>\r\n                {\r\n                    tracks.map(track => {\r\n                        const { id, name, album, preview_url } = track;\r\n                        return(\r\n                            <div key={ id } className='track-div' onClick={this.playAudio(preview_url)}>\r\n                                <img title={ name } className='album-images' alt='Album Image' src={album.images[0].url } />\r\n                                <p className='track-text'>{name}</p>\r\n                                <p className='track-icon'>{this.trackIcon(track)}</p>\r\n                            </div>\r\n                        )\r\n                    })\r\n                }\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default Tracks;"},"lineCount":null}},"error":null,"hash":"bcba9bb63709022e09923896bd769707","cacheData":{"env":{}}}